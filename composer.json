{
  "name": "comertis/timber",
  "description": "A PHP Logging library",
  "type": "library",
  "readme": "README.md",
  "license": "MIT",
  "keywords": [
    "logging"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Cristian Moraru",
      "email": "cristian@comertis.com"
    }
  ],
  "support": {
    "email": "support@comertis.com",
    "docs": "https://github.com/Comertis/Timber/tree/master/docs",
    "issues": "https://github.com/Comertis/Timber/issues",
    "source": "https://github.com/Comertis/Timber"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Comertis\\Timber": "src/",
      "Comertis\\Timber\\Internal": "src/internal/",
      "Comertis\\Timber\\Internal\\Engine": "src/internal/engine/",
      "Comertis\\Timber\\Internal\\Files": "src/internal/files/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Comertis\\Timber\\Tests": "tests/"
    }
  },
  "require": {
    "php": "7.1.* || 7.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  }
}
